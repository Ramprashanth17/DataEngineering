{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "AzureDE-ADF-Practice"
		},
		"DataLoading_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'DataLoading'"
		},
		"SQLDB_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'SQLDB'"
		},
		"DataLoading_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://cookbookdeaz.dfs.core.windows.net/"
		},
		"SQLDB_properties_typeProperties_server": {
			"type": "string",
			"defaultValue": "adfpracticecookbook.database.windows.net"
		},
		"SQLDB_properties_typeProperties_database": {
			"type": "string",
			"defaultValue": "sampleadf"
		},
		"SQLDB_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "sqladmin"
		},
		"NewFileTrigger_properties_typeProperties_scope": {
			"type": "string",
			"defaultValue": "/subscriptions/2b6a33e8-d4da-41b7-89d2-2f42df6fbc47/resourceGroups/azurepractice/providers/Microsoft.Storage/storageAccounts/cookbookdeaz"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "AzureDE-ADF-Practice_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2024-03-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"DataLoading_accountKey": {
						"value": "[parameters('DataLoading_accountKey')]"
					},
					"SQLDB_password": {
						"value": "[parameters('SQLDB_password')]"
					},
					"DataLoading_properties_typeProperties_url": {
						"value": "[parameters('DataLoading_properties_typeProperties_url')]"
					},
					"SQLDB_properties_typeProperties_server": {
						"value": "[parameters('SQLDB_properties_typeProperties_server')]"
					},
					"SQLDB_properties_typeProperties_database": {
						"value": "[parameters('SQLDB_properties_typeProperties_database')]"
					},
					"SQLDB_properties_typeProperties_userName": {
						"value": "[parameters('SQLDB_properties_typeProperties_userName')]"
					},
					"NewFileTrigger_properties_typeProperties_scope": {
						"value": "[parameters('NewFileTrigger_properties_typeProperties_scope')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}